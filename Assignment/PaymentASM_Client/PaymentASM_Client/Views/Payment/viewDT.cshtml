@model IEnumerable<PaymentASM_Client.Models.Doitac>

@{
    ViewBag.Title = "Thông tin đối tác";
}

<h2>Thông tin đối tác</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaDT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MKDT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaTK)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaDT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MKDT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaTK)
        </td>
    </tr>
}

</table>
